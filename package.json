{
  "name": "babel-boilerplate",
  "version": "0.0.13",
  "description": "Boilerplate with Babel for es6 projects.",
  "bin": {
    "es6-babel-generate": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "dev": "babel src --out-dir dist --presets es2015 --source-maps inline --watch",
    "build": "babel src --out-dir dist --presets es2015 --source-maps inline",
    "prepublish": "npm run build",
    "prestart": "npm run build"
  },
  "keywords": [
    "js",
    "gulp",
    "babel",
    "boilerplace",
    "es6",
    "es5"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eldino/babel-boilerplate.git"
  },
  "author": "Leonid Kukuyev <leonidkukuyev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "ncp": "^2.0.0",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0"
  }
}
